import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        /* Enter your code here. Read input from STDIN. Print output to STDOUT. Your class should be named Solution. */
        Scanner obj=new Scanner(System.in);
        int a=obj.nextInt();
        int b=obj.nextInt();
        char sy=obj.next().charAt(0);
        switch(sy){
            case '+':
                float sum=(float)a+b;
                System.out.printf("Addition of two number is %.1f",sum);
                break;
            case '-':
                float sub=(float)a-b;
                System.out.printf("Subtraction of two number is %.1f",sub);
                break;
            case '*':
                float pro=(float)a*b;
                System.out.printf("Multiplication of two number is %.1f",pro);
                break;
            case '/':
                float div=(float)a/b;
                System.out.printf("Division of two number is %.1f",div);
                break;
            case '%':
                float mod=(float)a%b;
                System.out.printf("Modulo of two number is %.1f",mod);
                break;
            default:
                System.out.println("Invalid Input");
        }
    }
}
